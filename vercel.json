{
    "version": 2,
    "builds": [{
        "src": "cmd/api/main.go",
        "use": "@vercel/go"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "cmd/api/main.go"
    }],
    "env": {
        "GO_VERSION": "1.21"
    },
    "functions": {
        "cmd/api/main.go": {
            "maxDuration": 30
        }
    }
}